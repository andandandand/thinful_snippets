   "source": [
    "The ID's and durations for all trips of duration greater than 500, ordered by duration."
   ]
  },
  {
   "cell_type": "raw",
   "metadata": {},
   "source": [
    "SELECT \n",
    "\ttrip_id,\n",
    "\tduration\n",
    "FROM\n",
    "\ttrips\n",
    "WHERE \n",
    "\tduration > 500\n",
    "ORDER BY duration DESC"
   ]
  },
  {
